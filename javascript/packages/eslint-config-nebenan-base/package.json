{
  "name": "eslint-config-nebenan-base",
  "description": "Shareable GoodHood-EU styleguide config for eslint",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": "goodhood-eu/styleguide",
  "bugs": "https://github.com/goodhood-eu/styleguide/issues",
  "version": "1.0.0",
  "keywords": [
    "linter",
    "config",
    "eslint"
  ],
  "main": "index.js",
  "engines": {
    "node": "^10.x.x"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "eslint:valid": "node ../test.js ./test/valid.es",
    "eslint:invalid": "node ../test.js ./test/invalid.es",
    "test": "npm run eslint:valid && npm run eslint:invalid"
  },
  "devDependencies": {
    "eslint": "^6.4.0"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.17.1"
  },
  "peerDependencies": {
    "eslint": "^6.4.0"
  }
}
